                                "mime": "image\/png",
                                "size": 50444,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/earl_warren.png"
                            },
                            "length_of_service": 5740,
                            "identifier": "earl_warren"
                        },
                        "text_blocks": [
                            {
                                "start": 633.761,
                                "stop": 637.727,
                                "byte_start": 2563803,
                                "byte_stop": 2579477,
                                "text": "We\u0027ll recess now, Mr. --"
                            }
                        ]
                    }
