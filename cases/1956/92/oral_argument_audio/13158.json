                        "text_blocks": [
                            {
                                "start": 0,
                                "stop": 18.954,
                                "byte_start": 103424,
                                "byte_stop": 179284,
                                "text": "Number 92, Schware versus Board of Bar Examiners of New, Mexico."
                            },
                            {
                                "start": 18.954,
                                "stop": 20.93,
                                "byte_start": 179284,
                                "byte_stop": 187225,
                                "text": "Mr. Levy."
                            }
                        ]
                    },
                    {
                        "start": 20.93,
                        "stop": 59.854,
                        "byte_start": 187225,
                        "byte_stop": 342915,
                        "speaker": {
                            "ID": 27460,
                            "name": "Herbert Monte Levy",
                            "href": "https:\/\/api.oyez.org\/people\/herbert_monte_levy",
                            "last_name": "Levy",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "herbert_monte_levy"
                        },
                        "text_blocks": [
                            {
                                "start": 20.93,
                                "stop": 24.753,
                                "byte_start": 187225,
                                "byte_stop": 202480,
                                "text": "Your Honor, may it please the Court."
                            },
                            {
                                "start": 24.753,
                                "stop": 37.584,
                                "byte_start": 202480,
                                "byte_stop": 253785,
                                "text": "I, of course, I think may not go into the broad particular questions that are presented here since this Court knows very well why I had placed the case on the calendar immediately following here in express."
                            },
                            {
                                "start": 37.584,
                                "stop": 49.35,
                                "byte_start": 253785,
                                "byte_stop": 300910,
                                "text": "The crucial difference of course is that the Schware case involves what happens to the man after he makes answer to the inquiries which had been made of him."
                            },
                            {
                                "start": 49.35,
                                "stop": 51.713,
                                "byte_start": 300910,
                                "byte_stop": 310314,
                                "text": "In fact, there\u0027s one step further."
                            },
                            {
                                "start": 51.713,
                                "stop": 59.474,
                                "byte_start": 310314,
                                "byte_stop": 341347,
                                "text": "This man never had any inquiries made of him in regard to a number of the facts which he disclosed."
                            },
                            {
                                "start": 59.474,
                                "stop": 59.854,
                                "byte_start": 341347,
                                "byte_stop": 342915,
                                "text": "The first --"
                            }
                        ]
                    },
                    {
                        "start": 59.854,
                        "stop": 71.578,
                        "byte_start": 342915,
                        "byte_stop": 389831,
                        "speaker": {
                            "ID": 15050,
                            "name": "Felix Frankfurter",
                            "href": "https:\/\/api.oyez.org\/people\/felix_frankfurter",
                            "last_name": "Frankfurter",
                            "roles": [
                                {
                                    "id": 2694,
                                    "type": "scotus_justice",
                                    "date_start": -975780000,
                                    "date_end": -231793200,
                                    "appointing_president": "Franklin D. Roosevelt",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2694"
                                }
                            ],
                            "thumbnail": {
                                "id": 32706,
                                "mime": "image\/png",
                                "size": 45083,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/felix_frankfurter.png"
                            },
                            "length_of_service": 8611,
                            "identifier": "felix_frankfurter"
                        },
                        "text_blocks": [
                            {
                                "start": 59.854,
                                "stop": 71.578,
                                "byte_start": 342915,
                                "byte_stop": 389831,
                                "text": "The first agreement to the rest of the five minutes, you summarize what you can achieve the meaning of showing up this record whether this (Inaudible)"
                            }
                        ]
                    },
                    {
                        "start": 71.578,
                        "stop": 81.206,
                        "byte_start": 389831,
                        "byte_stop": 428283,
                        "speaker": {
                            "ID": 27460,
                            "name": "Herbert Monte Levy",
                            "href": "https:\/\/api.oyez.org\/people\/herbert_monte_levy",
                            "last_name": "Levy",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "herbert_monte_levy"
                        },
                        "text_blocks": [
                            {
                                "start": 71.578,
                                "stop": 74.36,
                                "byte_start": 389831,
                                "byte_stop": 400906,
                                "text": "Because I think so, yes."
                            },
                            {
                                "start": 74.36,
                                "stop": 81.206,
                                "byte_start": 400906,
                                "byte_stop": 428283,
                                "text": "I would say, sir, that the crucial point here is that there is really no question of burden of proof involved."
                            }
                        ]
                    },
                    {
                        "start": 81.206,
                        "stop": 81.865,
                        "byte_start": 428283,
                        "byte_stop": 430895,
                        "speaker": {
                            "ID": 15050,
                            "name": "Felix Frankfurter",
                            "href": "https:\/\/api.oyez.org\/people\/felix_frankfurter",
                            "last_name": "Frankfurter",
                            "roles": [
                                {
                                    "id": 2694,
                                    "type": "scotus_justice",
                                    "date_start": -975780000,
                                    "date_end": -231793200,
                                    "appointing_president": "Franklin D. Roosevelt",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2694"
                                }
                            ],
                            "thumbnail": {
                                "id": 32706,
                                "mime": "image\/png",
                                "size": 45083,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/felix_frankfurter.png"
                            },
                            "length_of_service": 8611,
                            "identifier": "felix_frankfurter"
                        },
                        "text_blocks": [
                            {
                                "start": 81.206,
                                "stop": 81.865,
                                "byte_start": 428283,
                                "byte_stop": 430895,
                                "text": "A what?"
                            }
                        ]
                    },
                    {
                        "start": 81.865,
                        "stop": 108.812,
                        "byte_start": 430895,
                        "byte_stop": 538728,
                        "speaker": {
                            "ID": 27460,
                            "name": "Herbert Monte Levy",
                            "href": "https:\/\/api.oyez.org\/people\/herbert_monte_levy",
                            "last_name": "Levy",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "herbert_monte_levy"
                        },
                        "text_blocks": [
                            {
                                "start": 81.865,
                                "stop": 85.536,
                                "byte_start": 430895,
                                "byte_stop": 445628,
                                "text": "There\u0027s no question of burden of proof involved."
                            },
                            {
                                "start": 85.536,
                                "stop": 105.241,
                                "byte_start": 445628,
                                "byte_stop": 524413,
                                "text": "The court below relied entirely upon the fact that Schware had been a member of the Communist Party from 1934 to 1940, together with what they called his former actions in the use of aliases and what they termed his record of arrest and his attitudes there are."
                            },
                            {
                                "start": 105.241,
                                "stop": 108.812,
                                "byte_start": 524413,
                                "byte_stop": 538728,
                                "text": "This was the only point upon which they based their decision."
                            }
                        ]
                    },
                    {
                        "start": 108.812,
                        "stop": 111.574,
                        "byte_start": 538728,
                        "byte_stop": 549804,
                        "speaker": {
                            "ID": 15050,
                            "name": "Felix Frankfurter",
                            "href": "https:\/\/api.oyez.org\/people\/felix_frankfurter",
                            "last_name": "Frankfurter",
                            "roles": [
                                {
                                    "id": 2694,
                                    "type": "scotus_justice",
                                    "date_start": -975780000,
                                    "date_end": -231793200,
                                    "appointing_president": "Franklin D. Roosevelt",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2694"
                                }
                            ],
                            "thumbnail": {
                                "id": 32706,
                                "mime": "image\/png",
                                "size": 45083,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/felix_frankfurter.png"
                            },
                            "length_of_service": 8611,
                            "identifier": "felix_frankfurter"
                        },
                        "text_blocks": [
                            {
                                "start": 108.812,
                                "stop": 111.574,
                                "byte_start": 538728,
                                "byte_stop": 549804,
                                "text": "1934 to 1940?"
                            }
                        ]
                    },
                    {
                        "start": 111.574,
                        "stop": 116.233,
                        "byte_start": 549804,
                        "byte_stop": 568404,
                        "speaker": {
                            "ID": 27460,
                            "name": "Herbert Monte Levy",
                            "href": "https:\/\/api.oyez.org\/people\/herbert_monte_levy",
                            "last_name": "Levy",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "herbert_monte_levy"
                        },
                        "text_blocks": [
                            {
                                "start": 111.574,
                                "stop": 114.698,
                                "byte_start": 549804,
                                "byte_stop": 562239,
                                "text": "1934 to 1940."
                            },
                            {
                                "start": 114.698,
                                "stop": 116.233,
                                "byte_start": 562239,
                                "byte_stop": 568404,
                                "text": "Now, Schware presented --"
                            }
                        ]
                    },
                    {
                        "start": 116.233,
                        "stop": 125.541,
                        "byte_start": 568404,
                        "byte_stop": 605602,
                        "speaker": {
                            "ID": 15050,
                            "name": "Felix Frankfurter",
                            "href": "https:\/\/api.oyez.org\/people\/felix_frankfurter",
                            "last_name": "Frankfurter",
                            "roles": [
                                {
                                    "id": 2694,
                                    "type": "scotus_justice",
                                    "date_start": -975780000,
                                    "date_end": -231793200,
                                    "appointing_president": "Franklin D. Roosevelt",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2694"
                                }
                            ],
                            "thumbnail": {
                                "id": 32706,
                                "mime": "image\/png",
                                "size": 45083,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/felix_frankfurter.png"
                            },
                            "length_of_service": 8611,
                            "identifier": "felix_frankfurter"
                        },
                        "text_blocks": [
                            {
                                "start": 116.233,
                                "stop": 117.828,
                                "byte_start": 568404,
                                "byte_stop": 574777,
                                "text": "It\u0027s not uncontroversy."
                            },
                            {
                                "start": 117.828,
                                "stop": 125.541,
                                "byte_start": 574777,
                                "byte_stop": 605602,
                                "text": "It\u0027s not moved yet, but I -- it\u0027s the content as of the time of the inquiry, wasn\u0027t it?"
                            }
                        ]
                    },
                    {
                        "start": 125.541,
                        "stop": 159.213,
                        "byte_start": 605602,
                        "byte_stop": 740289,
                        "speaker": {
                            "ID": 27460,
                            "name": "Herbert Monte Levy",
                            "href": "https:\/\/api.oyez.org\/people\/herbert_monte_levy",
                            "last_name": "Levy",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "herbert_monte_levy"
                        },
                        "text_blocks": [
                            {
                                "start": 125.541,
                                "stop": 132.226,
                                "byte_start": 605602,
                                "byte_stop": 632351,
                                "text": "No, the Court itself talks in terms of past membership throughout."
                            },
                            {
                                "start": 132.226,
                                "stop": 137.2,
                                "byte_start": 632351,
                                "byte_stop": 652309,
                                "text": "The Bar Examiners, the Board of Bar Examiners talked in terms of past membership."
                            },
                            {
                                "start": 137.2,
                                "stop": 149.784,
                                "byte_start": 652309,
                                "byte_stop": 702568,
                                "text": "The only thing that is brought up to cast any doubt is in terms of when he left the Party with that particular 1944 letter, which is read in the 1955 context."
                            },
                            {
                                "start": 149.784,
                                "stop": 155.502,
                                "byte_start": 702568,
                                "byte_stop": 725452,
                                "text": "I don\u0027t know whether I should get into that particular problem now but I suppose I might as well."
                            },
                            {
                                "start": 155.502,
                                "stop": 159.213,
                                "byte_start": 725452,
                                "byte_stop": 740289,
                                "text": "The only point that was made in --"
                            }
                        ]
                    },
                    {
                        "start": 159.213,
                        "stop": 168.731,
                        "byte_start": 740289,
                        "byte_stop": 778428,
                        "speaker": {
                            "ID": 15050,
                            "name": "Felix Frankfurter",
                            "href": "https:\/\/api.oyez.org\/people\/felix_frankfurter",
                            "last_name": "Frankfurter",
                            "roles": [
                                {
                                    "id": 2694,
                                    "type": "scotus_justice",
                                    "date_start": -975780000,
                                    "date_end": -231793200,
                                    "appointing_president": "Franklin D. Roosevelt",
                                    "role_title": "Associate Justice of the Supreme Court of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2694"
                                }
                            ],
                            "thumbnail": {
                                "id": 32706,
                                "mime": "image\/png",
                                "size": 45083,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/felix_frankfurter.png"
                            },
                            "length_of_service": 8611,
                            "identifier": "felix_frankfurter"
                        },
                        "text_blocks": [
                            {
                                "start": 159.213,
                                "stop": 168.731,
                                "byte_start": 740289,
                                "byte_stop": 778428,
                                "text": "But it represent -- assume the word to be whether he was a Communist or they have terminated his membership in 1944 rather than 1940."
                            }
                        ]
                    },
                    {
                        "start": 168.731,
                        "stop": 295.418,
                        "byte_start": 778428,
                        "byte_stop": 1285099,
                        "speaker": {
                            "ID": 27460,
                            "name": "Herbert Monte Levy",
                            "href": "https:\/\/api.oyez.org\/people\/herbert_monte_levy",
                            "last_name": "Levy",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "herbert_monte_levy"
                        },
                        "text_blocks": [
                            {
                                "start": 168.731,
                                "stop": 171.174,
                                "byte_start": 778428,
                                "byte_stop": 788146,
                                "text": "I think that\u0027s the most that they could assume."
                            },
                            {
                                "start": 171.174,
                                "stop": 179.896,
                                "byte_start": 788146,
                                "byte_stop": 823045,
                                "text": "Now, I do want to point out again that there is really no controversy over the facts here or no question of burden of proof."
                            },
                            {
                                "start": 179.896,
                                "stop": 198.658,
                                "byte_start": 823045,
                                "byte_stop": 898069,
                                "text": "Because in the petition to review the denial of Schware\u0027s application, the statement is made that all the questions that were asked in the application length and at the first hearing were answered correctly."
                            },
                            {
                                "start": 198.658,
                                "stop": 213.846,
                                "byte_start": 898069,
                                "byte_stop": 958882,
                                "text": "And more importantly, beginning in pages 90 through 92, a defendant board admits or rather refuses to deny any complete truthfulness and so forth in the matters that Schware testified to."
                            },
                            {
                                "start": 213.846,
                                "stop": 231.766,
                                "byte_start": 958882,
                                "byte_stop": 1030562,
                                "text": "They say that while they cannot admit or deny the completeness and the truthfulness and so on, they have not made any claim that petitioner answered any questions in his application other than fully and accurately, nor is the respondent\u0027s decision in the petitioner\u0027s case based upon any such claim."
                            },
                            {
                                "start": 231.766,
                                "stop": 241.087,
                                "byte_start": 1030562,
                                "byte_stop": 1067865,
                                "text": "They make the same statement again in reference to the hearing at the paragraph ending just before number seven on page 91."
                            },
                            {
                                "start": 241.087,
                                "stop": 253.754,
                                "byte_start": 1067865,
                                "byte_stop": 1118542,
                                "text": "And again, on page 92, it is said very clearly at the end of paragraph number 10 that the decision of the respondent in the petitioner\u0027s case is based upon facts disclosed by the petitioner himself."
                            },
                            {
                                "start": 253.754,
                                "stop": 289.562,
                                "byte_start": 1118542,
                                "byte_stop": 1261693,
                                "text": "So that I think here we have no question of veracity, no question of burden of proof and the simple issue in the case is whether the three factors has Communist Party membership in the popular front period from 1934 to 1940, the use of aliases for the particular reasons set forth here and the mere fact that this man had been arrested without any consideration again as to whether he was guilty or innocent of crimes, whether these three factors can constitutionally be the basis for a decision barring him from taking the bar examination."
                            },
                            {
                                "start": 289.562,
                                "stop": 295.418,
                                "byte_start": 1261693,
                                "byte_stop": 1285099,
                                "text": "I see that I have about 30 seconds left and I think this might be the point which the Chief Justice might want to adjourn."
                            }
                        ]
                    },
                    {
                        "start": 295.418,
                        "stop": 298.102,
                        "byte_start": 1285099,
                        "byte_stop": 1295862,
                        "speaker": {
                            "ID": 15099,
                            "name": "Earl Warren",
                            "href": "https:\/\/api.oyez.org\/people\/earl_warren",
                            "last_name": "Warren",
                            "roles": [
                                {
                                    "id": 2743,
                                    "type": "scotus_justice",
                                    "date_start": -512503200,
                                    "date_end": -16570800,
                                    "appointing_president": "Dwight D. Eisenhower",
                                    "role_title": "Chief Justice of the United States",
                                    "institution_name": "Supreme Court of the United States",
                                    "href": "https:\/\/api.oyez.org\/preson_role\/scotus_justice\/2743"
                                }
                            ],
                            "thumbnail": {
                                "id": 32704,
                                "mime": "image\/png",
                                "size": 50444,
                                "href": "https:\/\/api.oyez.org\/sites\/default\/files\/earl_warren.png"
                            },
                            "length_of_service": 5740,
                            "identifier": "earl_warren"
                        },
                        "text_blocks": [
                            {
                                "start": 295.418,
                                "stop": 298.102,
                                "byte_start": 1285099,
                                "byte_stop": 1295862,
                                "text": "Gladly."
                            }
                        ]
                    },
                    {
                        "start": 298.102,
                        "stop": 300.321,
                        "byte_start": 1295862,
                        "byte_stop": 1304639,
                        "speaker": {
                            "ID": 27460,
                            "name": "Herbert Monte Levy",
                            "href": "https:\/\/api.oyez.org\/people\/herbert_monte_levy",
                            "last_name": "Levy",
                            "roles": null,
                            "thumbnail": null,
                            "length_of_service": 0,
                            "identifier": "herbert_monte_levy"
                        },
                        "text_blocks": [
                            {
                                "start": 298.102,
                                "stop": 300.321,
                                "byte_start": 1295862,
                                "byte_stop": 1304639,
                                "text": "I hope I was --"
                            }
                        ]
                    }
                ]
            }
